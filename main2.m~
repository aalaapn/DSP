tic
bandlimits = [];
for i=1:50
    band = i*100;
    bandlimits = [bandlimits band];
end
[sig, fs] = audioread('songs/bagbak.wav');
sig = sig(:,1);
out = filterbank(sig);

hann = hannWindow(out);

diff = diffrect(hann);


down_low = downsample(hann, 100);

[ACF1,lags1,bounds1] = autocorr(down_low(:,1),int64(fs/100),[],2);
[ACF2,lags2,bounds2] = autocorr(down_low(:,2),int64(fs/100),[],2);
[ACF3,lags3,bounds3] = autocorr(down_low(:,3),int64(fs/100),[],2);
[ACF4,lags4,bounds4] = autocorr(down_low(:,4),int64(fs/100),[],2);
[ACF5,lags5,bounds5] = autocorr(down_low(:,5),int64(fs/100),[],2);
[ACF6,lags6,bounds6] = autocorr(down_low(:,6),int64(fs/100),[],2);

ACF = [ACF1 ACF2 ACF3 ACF4 ACF5 ACF6];
BPM_ACF = BPM_convert(ACF1, fs/100);

multi_plot(ACF, BPM_ACF, 40);




[~,locs]=findpeaks(ACF3);
locs
lagindex = mean(diff(locs)*0.01);

((fs/10000)*60)/lagindex;

a = [];
for i = 100:400
    a = [a ACF(i)];
end

maxi = max(a);

ind = find(ACF==maxi);

BPM = fs/100*60/locs;
ind = fs/100*60/82
toc
